                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT        orders.Order_Id, orders.Order_Region, orders.Order_Country, orders.Order_State, orders.Order_City, orders.Status, orders.Market, orders.Type_Transaction, order_item.Sales, order_item.Total, order_item.Discount, order_item.Profit_Ratio, orders.Order_Date, product.Product_Id, customer.Customer_Id, department.Department_Id, 
