                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT        shipping.Order_Id, shipping.Order_Date, shipping.Shipping_Date, shipping.Late_Delivery_Risk, shipping.Days_For_Shipment_Scheduled, shipping.Days_For_Shipping_Actual, shipping.Delivery_Status, shipping.Shipping_Mode, order_item.Order_Item_Id, product.Product_Id, department.Department_Id, orders.Customer_Id
